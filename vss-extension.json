{
    "manifestVersion": 1,
    "id": "install-bundletool-with-url",
    "name": "Bundletool Install Helper",
    "version": "1.3.2",
    "publisher": "AndreyPodgorodetskiy",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Fork of damienaicheh InstallBundletool",
    "icons": {
        "default": "icon.png"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "mobile",
        "android"
    ],
    "content": {
        "details": {
            "path": "OVERVIEW.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },
    "links": {
        "support": {
            "uri": "https://github.com/podgorodetskiy/azure-devops-bundletool/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/podgorodetskiy/azure-devops-bundletool"
    },
    "files": [
        {
            "path": "tasks/InstallBundletool"
        }
    ],
    "contributions": [
        {
            "id": "install-bundletool-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/InstallBundletool"
            }
        }
    ]
}